{
    "name": "book-store",
    "version": "1.0.0",
    "private": true,
    "description": "graphql starter tutorial",
    "main": "express.js",
    "scripts": {
        "server": "nodemon express.js --exec babel-node express --presets babel-preset-env --config nodemon.config.json",
        "build": "nodemon express.js --exec babel-node express --presets babel-preset-env --config nodemon.config.json"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": [
            "eslint",
            "cd client && jest --findRelatedTests"
        ],
        "*.{js,json,css,scss,md,babelrc,eslintrc,prettierrc}": [
            "prettier --write",
            "git add"
        ]
    },
    "keywords": [
        "node",
        "express",
        "graphql"
    ],
    "author": {
        "email": "code.review.0@gmail.com",
        "name": "Mustafa Alroomi",
        "url": "https://github.com/musti-91/graphql-books"
    },
    "license": "MIT",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.16.4",
        "express-graphql": "^0.9.0",
        "graphql": "^14.1.1",
        "lodash": "^4.17.11",
        "mongoose": "^5.8.1",
        "node-sass": "^4.13.0",
        "nodemon": "^2.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/node": "^7.7.4",
        "@babel/preset-env": "^7.9.5",
        "@babel/register": "^7.9.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.1.0",
        "babel-preset-env": "^1.7.0",
        "husky": "^3.1.0",
        "lint-staged": "^9.5.0"
    },
    "repository": {
        "url": "https://github.com/musti-91/graphql-books",
        "directory": "src",
        "type": "javascript"
    }
}